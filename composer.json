{
  "name": "localdisk/monar",
  "description": "read and write thread floating type bbs.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "localdisk",
      "email": "smoochynet@gmail.com"
    }
  ],
  "require": {
    "php" : "~8.0",
    "guzzlehttp/guzzle": "~7.0",
    "illuminate/support": "~8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~9.0",
    "orchestra/testbench": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Localdisk\\Monar\\": "src/Monar"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Localdisk\\Monar\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Localdisk\\Monar\\MonarProvider"
      ],
      "aliases": {
        "Monar": "Localdisk\\Monar\\Monar"
      }
    }
  },
  "minimum-stability": "stable"
}
